!! urxvt config {{{

URxvt.saveLines: 65535 
URxvt.scrollBar: false
!URxvt.foreground: white
!URxvt.background: black
!URxvt.background: [90]#090909
!URxvt.depth: 32
!URxvt.shading: 10
URxvt.secondaryScroll: true
URxvt.font: xft:Monospace:pixelsize=10
URxvt.perl-ext-common: mark-urls,tabbed
URxvt.urlLauncher: firefox

!! with tabs:
URxvt.tabbed: true
URxvt.tabbed.saveLines: 65535 
URxvt.tabbed.scrollBar: false
URxvt.tabbed.tabbar-fg: 7
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 12

!! URxvt font sizes
!! Bind C-0, C-+ and C-= to activate medium, big and small font size respectively.
URxvt.keysym.C-0: command:\033]710;xft:Monospace:pixelsize=10\007
URxvt.keysym.C-1: command:\033]710;xft:Monospace:pixelsize=12\007
URxvt.keysym.C-2: command:\033]710;xft:Monospace:pixelsize=14\007
URxvt.keysym.C-3: command:\033]710;xft:Monospace:pixelsize=16\007
URxvt.keysym.C-4: command:\033]710;xft:Monospace:pixelsize=18\007
URxvt.keysym.C-5: command:\033]710;xft:Monospace:pixelsize=20\007
URxvt.keysym.C-6: command:\033]710;xft:Monospace:pixelsize=22\007
URxvt.keysym.C-7: command:\033]710;xft:Monospace:pixelsize=24\007
URxvt.keysym.C-8: command:\033]710;xft:Monospace:pixelsize=26\007
URxvt.keysym.C-9: command:\033]710;xft:Monospace:pixelsize=28\007

!! }}}

